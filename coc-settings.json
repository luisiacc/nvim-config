{
    "tsserver.trace.server": "verbose",
    "pyright.trace.server": "verbose",
    "python.linting.pylintArgs": ["--extension-pkg-whitelist=PyQt5"],
    "diagnostic.displayByAle": false,
    "coc.preferences.formatOnType": false,
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.enableGlobalStatus": false,
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
    "git.changedSign.text": "▌",
    "git.removedSign.text": "▌",
    "git.addedSign.text": "▌",
    "git.changeRemovedSign.text": "▌",
    "git.topRemovedSign.text": "▌",
    "emmet.showSuggestionsAsSnippets": false,
    //explorer
    "explorer.width": 40,
    "explorer.file.showHiddenFiles": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.git.enable": true,
    "explorer.icon.enableVimDevicons": true,
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings.global": {
        "v": "open:vsplit"
    },
    "html.filetypes": [
        "html",
        "handlebars",
        "htmldjango",
        "django",
        "djangohtml"
    ],
    "html.format.enable": true,
    "tsserver.disableAutomaticTypeAcquisition": true,
    "suggest.completionItemKindLabels": {
        "keyword": "",
        "variable": "",
        "value": "",
        "operator": "Ψ",
        "constructor": "",
        "function": "ƒ",
        "reference": "渚",
        "constant": "",
        "method": "",
        "struct": "פּ",
        "class": "",
        "interface": "",
        "text": "",
        "enum": "",
        "enumMember": "",
        "module": "",
        "color": "",
        "property": "",
        "field": "料",
        "unit": "",
        "event": "鬒",
        "file": "",
        "folder": "",
        "snippet": "",
        "typeParameter": "",
        "default": ""
    },
    "pyright.disableSelfClsNotAccessed": true
}
